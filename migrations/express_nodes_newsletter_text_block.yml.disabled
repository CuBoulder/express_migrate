id: express_nodes_newsletter_text_block
label: Express Nodes Newsletter Section Content Migration
migration_tags:
  - express


source:
  plugin: url
  data_fetcher_plugin: file
  data_parser_plugin: simple_xml
  urls:
    - data.xml
  item_selector: /root/nodes/item/faqs/item/fields/field_newsletter_text_block/data/item/
  fields:


    - name: id
      label: 'id'
      selector: 'id'

    - name: field_newsletter_block_body
      label: 'field_newsletter_block_body'
      selector: 'collection/field_qa_answer/data/item/field_qa_answer_value'

    - name: field_newsletter_block_title
      label: 'field_newsletter_block_title'
      selector: 'collection/field_qa_answer/data/item/field_qa_answer_format'


  ids:
    id:
      type: string
process:



destination:
  plugin: 'entity_reference_revisions:paragraph'
  default_bundle: 'ucb_newsletter_text_block'


migration_dependencies:
  required:
    - express_media_images


dependencies:
  enforced:
    module:
      - migrate_express
